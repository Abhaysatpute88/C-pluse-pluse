

#include <iostream>

using namespace std;

 main()
{
    int age;
    cout<<"Enter your age\n";
    
    cin>>age;
    
    


switch (age)
{case 12:
cout<<"you are 12 year old";
    break;
case 18:
    cout<<"you are 18 year old";
    break;

default:
    cout<<"you are neither 12 nor 18 ";
    break;
}

}    
